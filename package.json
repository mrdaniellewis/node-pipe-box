{
  "name": "pipe-box",
  "version": "2.0.5",
  "description": "Wrap a series of piped streams into a single stream",
  "main": "index.js",
  "files": [],
  "scripts": {
    "lint": "eslint *.js",
    "jest": "jest",
    "test": "npm run lint && npm run jest"
  },
  "keywords": [
    "pipe",
    "transform",
    "stream"
  ],
  "author": "Daniel Lewis",
  "repository": "https://github.com/mrdaniellewis/node-pipe-box.git",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0",
    "stream-collect": "^2.1.5"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "no-underscore-dangle": "off",
      "no-cond-assign": [
        "error",
        "except-parens"
      ],
      "strict": ["error", "global"]
    }
  },
  "jest": {
    "clearMocks": true
  }
}
